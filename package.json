    "gulp-install": "^1.1.0",
    "gulp-notify": "^4.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-wp-pot": "^2.4.3",
    "gulp-zip": "^4.2.0",
    "sass": "^1.44.0"
  },
  "dependencies": {
    "gulp-uglify": "^3.0.2",
    "gulp-update-version": "^1.0.2",
    "npm-run-all": "^4.1.5"
